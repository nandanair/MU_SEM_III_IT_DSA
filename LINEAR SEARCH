#include<stdio.h>
int main()
{
int arr[100];
int n,i,val,found=0;
printf("\n enter number of elements in array:");
scanf("%d",&n);
printf("\n enter elements :");
for(i=0;i<n;i++)
{
    scanf("\n%d",&arr[i]);
}
printf("\n enter element to be searched:");
scanf("%d",&val);
for(i=0;i<n;i++)
{
    if(arr[i]==val)
    {
     found=1;
     break;
    }

}
if(found==0)
    printf("\n element not found");
else
    printf("\n element found at location %d",i+1);
return 0;
}


